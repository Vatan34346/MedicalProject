<div class="container">
    <div class="register">
        <form action="/admin/register" method="post">
              <h1>რეგისტრაცია</h1><br>
              <input type="text" placeholder="სახელი" name="name" id="name" required><br>

              <input type="email" placeholder="მაილი" name="email" id="email" required><br>
          
              <input type="password" placeholder="პაროლი" name="pass" id="psw" required><br><br>
          
              <button type="submit" class="registerbtn">რეგისტრაცია</button>
        </form>
    </div>
</div>
